        "#Calculating Accuracy, Precision, Recall and F-Score"
      ],
      "id": "-fTHZz3wnb-r"
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "tSMN_n4-U7Gh"
      },
      "source": [
        "pred = model.predict(X_val)"
      ],
      "id": "tSMN_n4-U7Gh",
      "execution_count": 14,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "DqzrZCTBVsAu"
      },
      "source": [
        "pred = pred>=0.5\n",
        "pred = pred - 0.0"
      ],
      "id": "DqzrZCTBVsAu",
      "execution_count": 15,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "t06xBeBDpjz2"
      },
      "source": [
        "y_val = y_val-0.0"
      ],
      "id": "t06xBeBDpjz2",
      "execution_count": 16,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "ObfK3NIfWJyM"
      },
      "source": [
        "true_positives = 0\n",
        "false_positives = 0\n",
        "false_negatives = 0\n",
        "for i in range(pred.shape[0]):\n",
        "  for j in range(pred.shape[1]):\n",
        "    for k in range(pred.shape[2]):\n",
        "      if y_val[i][j][k] == 1.0 and pred[i][j][k] == 1.0:\n",
        "        true_positives+=1                                \n",
        "      elif y_val[i][j][k] == 1.0 and pred[i][j][k] == 0.0:\n",
        "        false_negatives+=1\n",
        "      elif y_val[i][j][k] == 0.0 and pred[i][j][k] == 1.0:\n",
        "        false_positives+=1\n",
        "accuracy = (true_positives/(true_positives+false_positives+false_negatives))\n",
        "precision = true_positives/(true_positives+false_positives)\n",
        "recall = true_positives/(true_positives+false_negatives)\n",
        "fscore = (2*precision*recall)/(precision+recall)"
      ],
      "id": "ObfK3NIfWJyM",
      "execution_count": 17,
      "outputs": []
    },
