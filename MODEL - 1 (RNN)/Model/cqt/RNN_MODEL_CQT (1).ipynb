      "source": [
        "# path to numpy files for data\n",
        "X_train = np.load(\"/content/drive/MyDrive/Data/totalCQT3.npy\")\n",
        "y_train = np.load(\"/content/drive/MyDrive/Data/totallabel3.npy\")\n",
